{{#if isValid}}
<div>
    {{#each docs}}
    <div>
        <p>Title: {{this.title}} &nbsp; Description: {{this.description}}</p>
        <p>Price: {{this.price}} &nbsp; Thumbnails: {{this.thumbnails}}</p>
        <p>Stock: {{this.stock}} &nbsp; Code: {{this.code}}</p>
        <button type="button">Add to car</button>
    </div>
    <br/>
    {{/each}}
</div>
<br/>
<div>
    {{#if hasPrevPage}}
        <a href={{this.prevLink}}><< Anterior</a>
    {{/if}}
    &nbsp;
    {{this.page}}
    &nbsp;
     {{#if hasNextPage}}
        <a href={{this.nextLink}}>Siguiente >></a>
    {{/if}} 
</div>
{{else}}
<h1>No hay productos para mostrar</h1>
{{/if}}